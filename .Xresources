xterm*background: 
aterm*loginShell:true
aterm*transparent:true
aterm*shading:60
aterm*background:Black
aterm*foreground:White
aterm*scrollBar:true
aterm*scrollBar_right:true
aterm*transpscrollbar:true
aterm*saveLines:32767
aterm*font:*-*-fixed-medium-r-normal--*-140-*-*-*-*-iso8859-1
aterm*boldFont:*-*-fixed-bold-r-normal--*-*-140-*-*-*-*-iso8859-1
URxvt*saveLines: 12000
URxvt*foreground: rgb:F7/F7/F7
!URxvt*foreground: rgb:FF/FF/FF
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0000/cccc
!URxvt.font: -*-terminus-medium-*-*-*-*-120-*-*-*-*-*-*
!URxvt.boldFont: -*-terminus-bold-*-*-*-*-120-*-*-*-*-*-*
!URxvt*font: xft:DejaVu Sans Mono:pixelsize=12:antialias=true:hinting=true
!URxvt*boldFont: xft:DejaVu Sans Mono:bold:pixelsize=12:antialias=true:hinting=true
!Rxvt*letterSpace: 0
! multiple fonts for both western and chinese
URxvt.font:       xft:Terminus:pixelsize=16:antialias=false:hinting=true, xft:SimSun:pixelsize=16:antialias=false:hinting=true
URxvt.boldFont:   xft:Terminus:bold:pixelsize=16:antialias=false:hinting=true, xft:SimSun:bold:pixelsize=16:antialias=false:hinting=true
!URxvt*italicFont: xft:Terminus (TTF):style=MediumItalic,pixelsize=12
!URxvt*color4: RoyalBlue
!URxvt*color12: RoyalBlue
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*scrollstyle: rxvt
URxvt*inputMethod: ibus
URxvt*tabbed.tabbar-fg: 8
URxvt*tabbed.tabbar-bg: 0
URxvt*tabbed.tab-fg:    0
!URxvt*tabbed.tab-bg:    12
URxvt*tabbed.tab-bg:    8
URxvt*geometry: 116x35
